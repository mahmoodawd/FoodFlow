<resources>

    <style name="Theme.FoodFlow" parent="Theme.Material3.Light.NoActionBar">
        <item name="colorPrimary">@color/theme_light_primary</item>
        <item name="colorOnPrimary">@color/theme_light_onPrimary</item>
        <item name="colorPrimaryContainer">@color/theme_light_primaryContainer</item>
        <item name="colorOnPrimaryContainer">@color/theme_light_onPrimaryContainer</item>
        <item name="colorSecondary">@color/theme_light_secondary</item>
        <item name="colorOnSecondary">@color/theme_light_onSecondary</item>
        <item name="colorSecondaryContainer">@color/theme_light_secondaryContainer</item>
        <item name="colorOnSecondaryContainer">@color/theme_light_onSecondaryContainer</item>
        <item name="colorTertiary">@color/theme_light_tertiary</item>
        <item name="colorOnTertiary">@color/theme_light_onTertiary</item>
        <item name="colorTertiaryContainer">@color/theme_light_tertiaryContainer</item>
        <item name="colorOnTertiaryContainer">@color/theme_light_onTertiaryContainer</item>
        <item name="colorError">@color/theme_light_error</item>
        <item name="colorErrorContainer">@color/theme_light_errorContainer</item>
        <item name="colorOnError">@color/theme_light_onError</item>
        <item name="colorOnErrorContainer">@color/theme_light_onErrorContainer</item>
        <item name="android:colorBackground">@color/theme_light_background</item>
        <item name="colorOnBackground">@color/theme_light_onBackground</item>
        <item name="colorSurface">@color/theme_light_surface</item>
        <item name="colorOnSurface">@color/theme_light_onSurface</item>
        <item name="colorSurfaceVariant">@color/theme_light_surfaceVariant</item>
        <item name="colorOnSurfaceVariant">@color/theme_light_onSurfaceVariant</item>
        <item name="colorOutline">@color/theme_light_outline</item>
        <item name="colorOnSurfaceInverse">@color/theme_light_inverseOnSurface</item>
        <item name="colorSurfaceInverse">@color/theme_light_inverseSurface</item>
        <item name="colorPrimaryInverse">@color/theme_light_inversePrimary</item>
    </style>

    <style name="Theme.FoodFlow.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.FoodFlow.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.FoodFlow.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>